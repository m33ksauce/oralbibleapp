{
  "name": "oralbiblemetadatatool",
  "description": "A tool for bundling Oral Bible metadata",
  "version": "0.0.0",
  "author": "Chris Meek @m33ksauce",
  "bin": {
    "oralbiblemetadatatool": "./bin/run"
  },
  "bugs": "https://github.com/m33ksauce/oralbiblemetadatatool/issues",
  "devDependencies": {
    "@oclif/dev-cli": "^1.26.0",
    "@oclif/test": "^1.2.8",
    "@types/chai": "^4.2.21",
    "@types/mocha": "^5.2.7",
    "@types/node": "^10.17.60",
    "chai": "^4.3.4",
    "eslint": "^5.16.0",
    "eslint-config-oclif": "^3.1.0",
    "eslint-config-oclif-typescript": "^0.1.0",
    "mocha": "^5.2.0",
    "nyc": "^14.1.1",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.10"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib"
  ],
